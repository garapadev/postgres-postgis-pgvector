# Configurações do PostgreSQL + PostGIS + pgvector
# Este arquivo pode ser usado com docker-compose --env-file config.env

# Versões das extensões
POSTGRES_VERSION=16.9
POSTGIS_VERSION=3.4.2
PGVECTOR_VERSION=0.7.4

# Configurações do banco
POSTGRES_DB=mydb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST_AUTH_METHOD=trust

# Configurações do PgAdmin
PGADMIN_DEFAULT_EMAIL=admin@exemplo.com
PGADMIN_DEFAULT_PASSWORD=admin

# Configurações da imagem Docker
DOCKER_IMAGE_NAME=garapadev/postgres-postgis-pgvector
DOCKER_TAG=16-optimized

# Portas
POSTGRES_PORT=5432
PGADMIN_PORT=8080
